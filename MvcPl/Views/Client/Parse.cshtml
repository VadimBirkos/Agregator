@model IEnumerable<Dal.Interface.PartyModel>

<div id="parse-results-list">
    <ul title="Все предприятия">
        @foreach (var item in Model)
        {
            <li>
                @item.Name
                @item.Email
                @if (item.Site != "Сайт отсутствует")
                {
                <a href="@item.Site">@item.Site</a>
                }
                else
                {
                @item.Site
                }
            </li>
        }
    </ul>
</div>